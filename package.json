    }
  ],
  "keywords": [
    "webeleon",
    "scss",
    "toolkit"
  ],
  "author": "Julien Prugne<juliene@webeleon.dev>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.6.2",
    "standard-version": "9.3.2"
  },
  "lint-staged": {
